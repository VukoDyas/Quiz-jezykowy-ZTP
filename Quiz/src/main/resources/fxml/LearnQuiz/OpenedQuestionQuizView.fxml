<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #2A2C2D;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <Text fx:id="questionText" fill="WHITE" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Treść pytania zamkniętego" textAlignment="CENTER" wrappingWidth="800.0">
        <font>
            <Font size="18.0" />
        </font>
    </Text>
    <TextField fx:id="answerTextField" layoutX="280.0" layoutY="135.0" prefHeight="40.0" prefWidth="241.0" />
    <Button fx:id="checkButton" layoutX="367.0" layoutY="200.0" mnemonicParsing="false" onAction="#check" style="-fx-background-color: #96CA2D;" text="Sprawdź" textFill="WHITE">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Button>
    <Label fx:id="respondLabel" contentDisplay="CENTER" layoutX="341.0" layoutY="313.0" prefHeight="40.0" prefWidth="118.0" textAlignment="CENTER" />
    <Button fx:id="returnButton" layoutX="14.0" layoutY="559.0" mnemonicParsing="false" onAction="#back" prefWidth="100.0" style="-fx-background-color: #96CA2D;" text="Powrót" textFill="WHITE">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Button>
    <Button fx:id="nextButton" layoutX="350.0" layoutY="450.0" mnemonicParsing="false" onAction="#next" prefWidth="100.0" style="-fx-background-color: #96CA2D;" text="Dalej" textFill="WHITE">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Button>
</Pane>

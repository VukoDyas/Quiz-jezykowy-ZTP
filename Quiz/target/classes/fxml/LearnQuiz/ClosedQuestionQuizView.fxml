<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<Pane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0"
      xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1">
    <Text fx:id="questionText" layoutX="299.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Treść pytania zamkniętego">
        <font>
            <Font size="18.0"/>
        </font>
    </Text>
    <Button fx:id="buttonA" layoutX="40.0" layoutY="140.0" mnemonicParsing="false" prefHeight="40.0"
            prefWidth="150.0" onAction="#checkAnswer" text="A."/>
    <Button fx:id="buttonB" layoutX="230.0" layoutY="140.0" mnemonicParsing="false" prefHeight="40.0"
            prefWidth="150.0" onAction="#checkAnswer" text="B."/>
    <Button fx:id="buttonC" layoutX="420.0" layoutY="140.0" mnemonicParsing="false" prefHeight="40.0"
            prefWidth="150.0" onAction="#checkAnswer" text="C."/>
    <Button fx:id="buttonD" layoutX="610.0" layoutY="140.0" mnemonicParsing="false" prefHeight="40.0"
            prefWidth="150.0" onAction="#checkAnswer" text="D."/>
    <Label fx:id="respondLabel" layoutX="254.0" layoutY="247.0" prefHeight="40.0" prefWidth="292.0"
           textAlignment="CENTER"/>
    <Button layoutX="14.0" layoutY="559.0" mnemonicParsing="false" onAction="#back"
            prefWidth="100.0" text="Powrót"/>
    <Button layoutX="350.0" layoutY="450.0" mnemonicParsing="false" onAction="#next"
            prefWidth="100.0" text="Dalej"/>
</Pane>
